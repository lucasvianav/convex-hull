doublyLinkedListsMain:
	gcc -g3 -fsanitize=address -fsanitize=undefined -fno-omit-frame-pointer -Wall -Werror -std=c99 doubly-linked-lists-main.c ../ADTs/list.c ../ADTs/list.h ../ADTs/stack.c ../ADTs/stack.h ../util.c ../util.h -o doubly-linked-lists-main
run-doublyLinkedListsMain:
	./doubly-linked-lists-main
